<?xml version="1.0"?>
<!DOCTYPE tournament [
<!ELEMENT tournament (match*)>

<!ATTLIST tournament tournament-name CDATA #REQUIRED 
start_date  CDATA #REQUIRED 
end_date CDATA #REQUIRED>

<!ELEMENT match (team+)>

<!ATTLIST match id CDATA #REQUIRED
